<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:background="#F2F2F2"
    android:layout_height="match_parent">
    <util.TitleBarUtils
        android:id="@+id/titleBar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/bigButtonHeight" >
    </util.TitleBarUtils>


<view.VerticalScrollView
    android:scrollbars="none"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:id="@+id/frag_home1"
    android:orientation="vertical"
    >
<view.SearchView
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

</view.SearchView>
    <include layout="@layout/index_home_list4" />

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#F2F2F2"
        />
    <include layout="@layout/index_home_list5"

        />

    <View
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="#F2F2F2"
        />



    <RelativeLayout

        android:layout_width="match_parent"
        android:background="#ffffffff"
        android:layout_height="100dp">
        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#F2F2F2"
            android:id="@+id/view" />

        <ImageView
            android:layout_width="180dp"
            android:layout_height="match_parent"
            android:id="@+id/imageView3"
            android:background="@mipmap/home_index"
            android:scaleType="fitXY"
            android:layout_below="@+id/view"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:textColor="#333333"
            android:text="为父母家人送上最珍贵的礼物"
            android:id="@+id/textView6"
            android:layout_above="@+id/textView7"
      />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="节日有"
            android:textColor="#333333"
            android:textSize="18sp"
            android:id="@+id/textView7"
            android:layout_centerVertical="true"
            android:layout_alignLeft="@+id/textView6"
            android:layout_alignStart="@+id/textView6" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:textColor="#008000"
            android:text="好礼"
            android:id="@+id/textView17"
            android:layout_alignBottom="@+id/textView7"
            android:layout_toRightOf="@+id/textView7"
            android:layout_toEndOf="@+id/textView7" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:text="送礼送"
            android:textColor="#333333"
            android:id="@+id/textView18"
            android:layout_below="@+id/textView7"
            android:layout_alignLeft="@+id/textView7"
            android:layout_alignStart="@+id/textView7" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:text="健康"
            android:textColor="#008000"
            android:id="@+id/textView19"
            android:layout_alignBottom="@+id/textView18"
            android:layout_toRightOf="@+id/textView18"
            android:layout_toEndOf="@+id/textView18" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:text="!"
            android:textColor="#333333"
            android:id="@+id/textView20"
            android:layout_alignBottom="@+id/textView19"
            android:layout_toRightOf="@+id/textView19"
            android:layout_toEndOf="@+id/textView19" />


    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#F2F2F2"
        />
   <TextView
       android:layout_width="match_parent"
       android:layout_marginTop="1dp"
       android:layout_marginBottom="1dp"
       android:background="#ffffffff"
       android:layout_height="25dp"
       android:textSize="18sp"
       android:textColor="#000000"
       android:text="健康咨询"
       />

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#F2F2F2"
        />
    <include layout="@layout/index_home_list5"/>
    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"></ListView>



</LinearLayout>

</view.VerticalScrollView>


</LinearLayout>